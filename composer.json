            "Osoobe\\Livewire\\BootstrapComponents\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osoobe\\Livewire\\BootstrapComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
